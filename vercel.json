{
  "buildCommand": "npm run build",
  "devCommand": "vite",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(?!.*\\.(?:js|css|png|jpg|jpeg|svg|gif|woff|woff2|ttf|eot|ico)$).*",
      "destination": "/index.html"
    }
  ]
}
